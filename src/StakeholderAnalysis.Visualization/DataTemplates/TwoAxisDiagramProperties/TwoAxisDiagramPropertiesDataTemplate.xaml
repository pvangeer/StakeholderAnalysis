<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:twoAxisDiagramProperties="clr-namespace:StakeholderAnalysis.Visualization.ViewModels.TwoAxisDiagramProperties">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary
            Source="/StakeholderAnalysis.Visualization;Component/DataTemplates/TreeViewDataTemplate.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <DataTemplate x:Key="TwoAxisDiagramPropertiesDataTemplate"
                  DataType="twoAxisDiagramProperties:TwoAxisDiagramPropertiesViewModel">
        <Grid>
            <StackPanel Orientation="Vertical">
                <ContentPresenter
                    Content="{Binding TreeViewModel}"
                    ContentTemplate="{StaticResource IPropertyCollectionItemDataTemplate}" />
            </StackPanel>
        </Grid>
    </DataTemplate>
</ResourceDictionary>