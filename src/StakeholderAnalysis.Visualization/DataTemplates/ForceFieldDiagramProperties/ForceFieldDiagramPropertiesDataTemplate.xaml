<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:forceFieldDiagramProperties="clr-namespace:StakeholderAnalysis.Visualization.ViewModels.ForceFieldDiagramProperties">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/StakeholderAnalysis.Visualization;Component/DataTemplates/TreeViewDataTemplate.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <DataTemplate x:Key="ForceFieldDiagramPropertiesDataTemplate" DataType="forceFieldDiagramProperties:ForceFieldDiagramPropertiesViewModel">
        <Grid>
            <StackPanel Orientation="Vertical">
                <ContentPresenter 
                    Content="{Binding TextsViewModel}" 
                    ContentTemplate="{StaticResource IPropertyCollectionItemDataTemplate}"/>
                <!--<ContentPresenter 
                    Content="{Binding BackgroundBrushViewModel}" 
                    ContentTemplate="{StaticResource IPropertyCollectionItemDataTemplate}"/>-->
            </StackPanel>
        </Grid>
    </DataTemplate>
</ResourceDictionary>